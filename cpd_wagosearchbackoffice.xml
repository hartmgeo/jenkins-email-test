      <codefragment><![CDATA[	private void createRow(final List<Div> children, final Div container)
	{
		final Div div = new Div();
		div.setSclass("ws-ip-row");
		final int width = 100 / children.size();

		for (final Div child : children)
		{
			child.setSclass("ws-ip-row-col");
			child.setStyle("float: left; width: " + width + "%;");
			div.appendChild(child);
		}

		container.appendChild(div);
	}]]></codefragment>
