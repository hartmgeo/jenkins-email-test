      <codefragment><![CDATA[	private String createPropertySettingsLabel(final SolrIndexedPropertyModel property)
	{
		final StringBuilder builder = new StringBuilder(42);
		builder.append(property.getType().getCode());
		if (property.isLocalized())
		{
			builder.append(" - localized");
		}
		if (property.isMultiValue())
		{
			builder.append(" - multiValue");
		}
		if (property.isFtsQuery())
		{
			builder.append(" - search");
		}
		if (property.isFacet())
		{
			builder.append(" - facet");
		}
		return builder.toString();
	}]]></codefragment>
